{
  "name": "pims",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev -p 3000",
    "build": "next build",
    "start": "next start -p 5000",
    "deploy": "next build && pm2 startOrRestart pm2.config.js --env prod",
    "deploy:server": "next build && PM2_HOME=/root/.pm2 pm2 startOrRestart pm2.config.js --env prod",
    "lint": "next lint",
    "lint:fix": "next lint --fix --no-ignore",
    "lint:quiet": "next lint --quiet",
    "lint:strict": "next lint --fix --max-warnings=0",
    "export": "next build && next export",
    "prepare": "husky",
    "pkg": "npx depcheck --config=.depcheckrc"
  },
  "dependencies": {
    "@formatjs/intl-relativetimeformat": "^11.4.5",

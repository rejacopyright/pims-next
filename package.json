    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "sass": "^1.81.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "app/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "next lint --fix --no-ignore"
    ]
  }
